{
  "files": [
    "typings/index.d.ts"
  ],
  "filesGlob": [
    "./build/**/*.ts",
    "./build/**/*.tsx",
    "!./node_modules/**"
  ],
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "es6",
    "module": "commonjs",
    "noLib": false,
    "preserveConstEnums": true,
    "declaration": false,
    "suppressImplicitAnyIndexErrors": true
  }
}
